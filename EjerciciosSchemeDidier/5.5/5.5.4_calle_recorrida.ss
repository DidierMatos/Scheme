(printf " cantidad de ganancia adquirida por calle recorrida")
(define (callerecor n)
  (cond
   [(= n 10 ) (display " 20 ventas hechas, El pago sera de :")(-(* 20 3000)480)]
   [(and (>= n 11 ) (<= n 30)) (display " 60 ventas realizadas, El pago va ser de :")(-(* 60 3000)480)]
   [(and (>= n 31 ) (<= n 100)) (display " 80 ventas realizadas, El pago va ser de :")(-(* 80 3000)480)]
   ))
(callerecor (read))
